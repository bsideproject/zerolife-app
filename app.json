{
  "expo": {
    "name": "Zerolife",
    "slug": "zero-life",
    "icon": "assets/icon.png",
    "sdkVersion": "44.0.0",
    "android": {
      "package": "com.ekrte.zerolife",
      "backgroundColor": "#1B1A1F",
      "permissions": ["CAMERA"],
      "splash": {
        "image": "assets/splash.png",
        "backgroundColor": "#1B1A1F"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "minSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ]
  }
}
